<h1>New interview</h1>

<div class="interview-show">
<div class="interview_info">
<%= form_for([@job_application, @interview]) do |f| %>
  <%= f.label :date %>
  <%= f.text_field :date %>
<p></p>
  <%= f.label :description %>
  <%= f.text_field :description %>
<p></p>
  <%= f.label :interviewer_name %>
  <%= f.text_field :interviewer_name %>
<p></p>
  <%= f.label :interviewer_email %>
  <%= f.text_field :interviewer_email %>
<br>
  <%= f.submit %>
</div>
</div>
<% end %>
